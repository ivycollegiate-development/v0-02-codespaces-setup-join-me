[31m-gameover () {[m
[31m-    echo "You see the corpse of a slain adventurer on the floor of the dungeon." > corpse[m
[31m-    echo "You find the following items on the corpse:  " >> corpse[m
[31m-    echo >> corpse[m
[31m-    echo $I >> corpse[m
[31m-    curr=`basename $PWD`[m
[31m-    while [ "$curr" != "entrance" ]; do[m
[31m-        pushd .. &> /dev/null[m
[31m-        dirs -c[m
[31m-        curr=`basename $PWD`[m
[31m-    done[m
[31m-    unset I[m
[31m-    unset HP[m
[31m-    $SHELL[m
[31m-}[m
[31m-roll() {[m
[31m-    echo "Enter a number."[m
[31m-    read NUM[m
[31m-    ALPHA=$RANDOM[m
[31m-    OMEGA=$RANDOM[m
[31m-    A=`echo $ALPHA | cut -b 1-2`[m
[31m-    O=`echo $OMEGA | cut -b 1-2`[m
[31m-    let "NUM=$ALPHA % $OMEGA"[m
[31m-    MONSTER=$RANDOM[m
[31m-    MONSTER=`echo $MONSTER | cut -b 1-2`[m
[31m-    NUM=`echo $NUM | cut -b 1-2`[m
[31m-    echo "The monster rolled " $MONSTER[m
[31m-    echo "You rolled " $NUM[m
[31m-    resolve[m
[31m-}[m
[31m-[m
[31m-resolve() {[m
